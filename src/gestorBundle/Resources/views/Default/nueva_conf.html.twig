{% extends '::base.html.twig' %}
{% block body %}
<!-- Page Heading -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Nueva configuracion
        </h1>
    </div>
</div>
<!-- /.row -->

{{ form_start(form) }}
<div class="formulario">
  <div class="form-group">
      {{ form_label(form.param) }}
      {{ form_widget(form.param, {'attr': {'class': 'form-control'}}) }}
      {{ form_errors(form.param) }}
  </div>

  <div class="form-group">
      {{ form_label(form.configuracion) }}
      {{ form_widget(form.configuracion, {'attr': {'class': 'form-control'}}) }}
      {{ form_errors(form.configuracion) }}
  </div>
  
  <div class="botones">
    {{ form_widget(form.guardar, {'attr': {'class': 'btn btn-info'}}) }}
    {{ form_widget(form.borrar, {'attr': {'class': 'btn btn-danger'}}) }}
  </div>

{{ form_end(form) }}
{% endblock %}
